{
  "ecmaFeatures": {
    "jsx": true
  },
  "env": {
    "browser": true,
    "node": true
  },
  "extends": ["airbnb", "prettier", "plugin:react-hooks/recommended"],
  "plugins": ["react"],
  "rules": {
    "import/no-unresolved": 0,
    "no-underscore-dangle": "off",
    "no-console": ["error", { allow: ["warn"] }]
  }
}
